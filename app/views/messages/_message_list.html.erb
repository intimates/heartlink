<table>
  <thead>
    <tr>
      <th>To uid</th>
      <th>From uid</th>
      <th>Body</th>
      <th>Raw body</th>
      <th>Pn value</th>
      <th>Opened at</th>
      <th>Sent at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% messages.each do |message| %>
      <tr>
        <td><%= message.to_uid %></td>
        <td><%= message.from_uid %></td>
        <td><%= message.body %></td>
        <td><%= message.raw_body %></td>
        <td><%= message.pn_value %></td>
        <td><%= message.opened_at %></td>
        <td><%= message.sent_at %></td>
        <td><%= link_to 'Show', message %></td>
        <td><%= link_to 'Edit', edit_message_path(message) %></td>
        <td><%= link_to 'Destroy', message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
